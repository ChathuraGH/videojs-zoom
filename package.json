{
  "name": "@theonlyducks/videojs-zoom",
  "title": "Video.js Zoom",
  "version": "1.0.1",
  "type": "module",
  "private": false,
  "description": "Simple plugin to zoom in video.js player",
  "author": "Giovane Santos <giovanesantos1999@gmail.com>",
  "license": "MIT",
  "main": "dist/videojs-zoom.cjs.js",
  "module": "dist/videojs-zoom.esm.js",
  "browser": "dist/videojs-zoom.js",
  "generator-videojs-plugin": {
    "version": "8.0.0"
  },
  "scripts": {
    "build": "yarn clean && yarn build:js && yarn build:css",
    "build:js": "rollup -c scripts/rollup.config.js",
    "build:css": "postcss -o dist/videojs-zoom.css --config scripts/postcss.config.js src/plugin.css",
    "clean": "rm -rf ./dist ./test/dist",
    "lint": "vjsstandard src/**/*.js",
    "start": "webpack serve --progress",
    "test": "karma start scripts/karma.config.js"
  },
  "engines": {
    "node": ">=16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theonlyducks/videojs-zoom-duck.git"
  },
  "bugs": {
    "url": "https://github.com/theonlyducks/videojs-zoom-duck/issues"
  },
  "vjsstandard": {
    "jsdoc": false
  },
  "videojs-plugin": {
    "style": "",
    "script": ""
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "videojs",
    "videojs-zoom-duck",
    "videojs-plugin",
    "videojs-plugin-zoom"
  ],
  "browserslist": [
    "defaults"
  ],
  "peerDependencies": {
    "video.js": ">= ^7"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.0.1",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "karma": "^6.4.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "rollup": "^3.3.0",
    "rollup-plugin-copy": "^3.4.0",
    "style-loader": "^3.3.1",
    "video.js": "^7.20.3",
    "videojs-generate-karma-config": "^8.0.1",
    "videojs-generate-postcss-config": "^2.1.1",
    "videojs-generate-rollup-config": "^7.0.0",
    "videojs-standard": "^9.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
